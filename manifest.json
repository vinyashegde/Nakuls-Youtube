{
  "name": "Nakuls Youtube",
  "version": "2.1.0.2",
  "manifest_version": 2,
  "description": "my friend Nakul can see youtube videos without ads",
  "browser_action": {
    "default_name": "Nakuls Youtube",
    "default_icon": "icon.png"
  },
   "icons": {
      "128": "youtube.png",
      "16": "youtube.png",
      "48": "youtube.png"
   },
  "permissions": ["tabs"],
	"background": {
		"scripts": ["background.js"]
	}
}
